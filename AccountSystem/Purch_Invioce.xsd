                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT        tbl_Account.ContactName, tbl_Account.ContactAddress, tbl_Account.Contact_no, tbl_Purchase.Purch_date, tbl_Purchase.Purch_bill_no,
                         tbl_Purchase.Purch_On, tbl_Product.Prod_name, tbl_Product.Barcode, SUM(tbl_Purchase_Detail.Prod_qty * ISNULL(tbl_Purchase_Detail.Pakg_id, 1)) AS QtyT,
                          tbl_Purchase_Detail.Prod_rate / ISNULL(tbl_Purchase_Detail.Pakg_id, 1) AS RateT, tbl_Unit.Unit_title, 1 AS SNO, tbl_Purchase.Remarks,
                         SUM(tbl_Purchase_Detail.Bns) AS Bns, SUM(tbl_Purchase_Detail.Disct) AS Disct
FROM            tbl_Purchase INNER JOIN
                         tbl_Purchase_Detail ON tbl_Purchase.Purch_id = tbl_Purchase_Detail.Purch_id INNER JOIN
                         tbl_Account ON tbl_Purchase.Contact_id = tbl_Account.Contact_id INNER JOIN
                         tbl_Product ON tbl_Purchase_Detail.Prod_id = tbl_Product.Prod_id INNER JOIN
                         tbl_Unit ON tbl_Product.Unit_id = tbl_Unit.Unit_id
WHERE        (tbl_Purchase.CompID = @Comp)
GROUP BY tbl_Account.ContactName, tbl_Account.ContactAddress, tbl_Account.Contact_no, tbl_Purchase.Purch_date, tbl_Purchase.Purch_bill_no,
                         tbl_Purchase.Purch_On, tbl_Product.Prod_name, tbl_Product.Barcode, tbl_Purchase_Detail.Prod_rate / ISNULL(tbl_Purchase_Detail.Pakg_id, 1),
                         tbl_Unit.Unit_title, tbl_Purchase.Remarks
HAVING        (tbl_Purchase.Purch_bill_no = @BillNo)</CommandText>
