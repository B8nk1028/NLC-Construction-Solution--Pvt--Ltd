          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="DataTable2TableAdapter" GeneratorDataComponentClassName="DataTable2TableAdapter" Name="DataTable2" UserDataComponentName="DataTable2TableAdapter">
            <MainSource>
              <DbSource ConnectionRef="DBConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT        ISNULL(V_Issuance.Issuance, 0) - ISNULL(V_Sales.Sales, 0) AS WithS, ISNULL(tbl_Account.ContactName, tbl_Account_1.ContactName) AS ContactName,
                         ISNULL(tbl_Product_1.Prod_name, tbl_Product.Prod_name) AS Prod_name
FROM            tbl_Account AS tbl_Account_1 INNER JOIN
                             (SELECT        SUM(tbl_SaleDetail.prod_qty) AS Sales, tbl_SaleDetail.prod_id, tbl_Sale.Contact_id
                               FROM            tbl_Sale INNER JOIN
                                                         tbl_SaleDetail ON tbl_Sale.Sale_id = tbl_SaleDetail.Sale_id
                               WHERE        (tbl_Sale.Sale_date &lt;= CONVERT(DATETIME, @D1, 102)) AND (tbl_Sale.CompID = @Comid)
                               GROUP BY tbl_SaleDetail.prod_id, tbl_Sale.Contact_id) AS V_Sales ON tbl_Account_1.Contact_id = V_Sales.Contact_id INNER JOIN
                         tbl_Product ON V_Sales.prod_id = tbl_Product.Prod_id FULL OUTER JOIN
                         tbl_Account INNER JOIN
                             (SELECT        SUM(TblIssueDTL.prod_qty) AS Issuance, TblIssueDTL.prod_id, tblIssue.Contact_id
                               FROM            tblIssue INNER JOIN
                                                         TblIssueDTL ON tblIssue.Issue_id = TblIssueDTL.Issue_ID
                               WHERE        (tblIssue.Issue_date &lt; CONVERT(DATETIME, @D1, 102)) AND (tblIssue.CompID = @Comid)
                               GROUP BY TblIssueDTL.prod_id, tblIssue.Contact_id) AS V_Issuance ON tbl_Account.Contact_id = V_Issuance.Contact_id INNER JOIN
                         tbl_Product AS tbl_Product_1 ON V_Issuance.prod_id = tbl_Product_1.Prod_id ON V_Sales.Contact_id = V_Issuance.Contact_id AND
                         V_Sales.prod_id = V_Issuance.prod_id
UNION
SELECT        ISNULL(V_Stock.Stock, 0) - ISNULL(V_WithS.WithS, 0) AS INHAND, 'In Hand' AS Titles, ISNULL(tbl_Product_2.Prod_name, tbl_Product_1.Prod_name)
                         AS Products
FROM            tbl_Product AS tbl_Product_1 INNER JOIN
                             (SELECT        SUM(V_Issuance_1.Issuance - ISNULL(V_Sales_2.Sales, 0)) AS WithS, V_Issuance_1.prod_id
                               FROM            (SELECT        SUM(tbl_SaleDetail_2.prod_qty) AS Sales, tbl_SaleDetail_2.prod_id
                                                         FROM            tbl_Sale AS tbl_Sale_2 INNER JOIN
                                                                                   tbl_SaleDetail AS tbl_SaleDetail_2 ON tbl_Sale_2.Sale_id = tbl_SaleDetail_2.Sale_id
                                                         WHERE        (tbl_Sale_2.Sale_date &lt;= CONVERT(DATETIME, @D1, 102)) AND (tbl_Sale_2.CompID = @Comid)
                                                         GROUP BY tbl_SaleDetail_2.prod_id) AS V_Sales_2 RIGHT OUTER JOIN
                                                             (SELECT        SUM(TblIssueDTL_1.prod_qty) AS Issuance, TblIssueDTL_1.prod_id
                                                               FROM            tblIssue AS tblIssue_1 INNER JOIN
                                                                                         TblIssueDTL AS TblIssueDTL_1 ON tblIssue_1.Issue_id = TblIssueDTL_1.Issue_ID
                                                               WHERE        (tblIssue_1.Issue_date &lt;= CONVERT(DATETIME, @D1, 102)) AND (tblIssue_1.CompID = @Comid)
                                                               GROUP BY TblIssueDTL_1.prod_id) AS V_Issuance_1 ON V_Sales_2.prod_id = V_Issuance_1.prod_id
                               GROUP BY V_Issuance_1.prod_id
                               HAVING         (SUM(V_Issuance_1.Issuance - ISNULL(V_Sales_2.Sales, 0)) &gt; 0)) AS V_WithS ON tbl_Product_1.Prod_id = V_WithS.prod_id FULL OUTER JOIN
                             (SELECT        V_Purch.Prod_id, V_Purch.Purchased - ISNULL(V_Sales_1.Sales, 0) AS Stock
                               FROM            (SELECT        SUM(tbl_Purchase_Detail.Prod_qty) AS Purchased, tbl_Purchase_Detail.Prod_id
                                                         FROM            tbl_Purchase INNER JOIN
                                                                                   tbl_Purchase_Detail ON tbl_Purchase.Purch_id = tbl_Purchase_Detail.Purch_id
                                                         WHERE        (tbl_Purchase.Purch_date &lt;= CONVERT(DATETIME, @D1, 102)) AND (tbl_Purchase.CompID = @Comid)
                                                         GROUP BY tbl_Purchase_Detail.Prod_id) AS V_Purch LEFT OUTER JOIN
                                                             (SELECT        SUM(tbl_SaleDetail_1.prod_qty) AS Sales, tbl_SaleDetail_1.prod_id
                                                               FROM            tbl_Sale AS tbl_Sale_1 INNER JOIN
                                                                                         tbl_SaleDetail AS tbl_SaleDetail_1 ON tbl_Sale_1.Sale_id = tbl_SaleDetail_1.Sale_id
                                                               WHERE        (tbl_Sale_1.Sale_date &lt;= CONVERT(DATETIME, @D1, 102)) AND (tbl_Sale_1.CompID = @Comid)
                                                               GROUP BY tbl_SaleDetail_1.prod_id) AS V_Sales_1 ON V_Purch.Prod_id = V_Sales_1.prod_id) AS V_Stock INNER JOIN
                         tbl_Product AS tbl_Product_2 ON V_Stock.Prod_id = tbl_Product_2.Prod_id ON V_WithS.prod_id = V_Stock.Prod_id</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="true" AutogeneratedName="D1" ColumnName="" DataSourceName="" DataTypeServer="date" DbType="DateTime" Direction="Input" ParameterName="@D1" Precision="0" ProviderType="DateTime" Scale="0" Size="3" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
