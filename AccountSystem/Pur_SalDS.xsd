                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        tbl_Sale.Sale_date, tbl_Sale.Sale_On, TblAccEntry.EntryAccName AS ContactName, TblAccEntry.BAddress AS ContactAddress, TblAccEntry.BContactNo AS Contact_no, tbl_SaleDetail.prod_qty, tbl_SaleDetail.Prod_rate, 
                         tbl_Unit.Unit_title, tbl_Product.Prod_name, tbl_Product.Barcode, 1 AS SNO, tbl_Sale.Sale_bill_no, tbl_Sale.Remarks, tbl_SaleDetail.Bns, tbl_SaleDetail.Disct, TblAccEntry_1.EntryAccName AS Expr1, 'Sale' AS Expr2
FROM            tbl_Sale INNER JOIN
                         tbl_SaleDetail ON tbl_Sale.Sale_id = tbl_SaleDetail.Sale_id INNER JOIN
                         tbl_Product ON tbl_SaleDetail.prod_id = tbl_Product.Prod_id INNER JOIN
                         tbl_Unit ON tbl_SaleDetail.Unit_id = tbl_Unit.Unit_id INNER JOIN
                         TblAccEntry ON tbl_Sale.Contact_id = TblAccEntry.EntryAccID INNER JOIN
                         TblAccEntry AS TblAccEntry_1 ON tbl_Sale.Salesman = TblAccEntry_1.EntryAccID
WHERE        ('S'+CONVERT(VARCHAR,tbl_Sale.Sale_id) = @BillNo) AND (tbl_Sale.BranchID = @Comp)
UNION
SELECT        tbl_Purchase.Purch_date, tbl_Purchase.Purch_On, TblAccEntry.EntryAccName AS ContactName, TblAccEntry.BAddress AS ContactAddress, TblAccEntry.BContactNo AS Contact_no, 
                         tbl_Purchase_Detail.Prod_qty * ISNULL(tbl_Purchase_Detail.Pakg_id, 1) AS QtyT, tbl_Purchase_Detail.Prod_rate / ISNULL(tbl_Purchase_Detail.Pakg_id, 1) AS RateT, tbl_Unit_1.Unit_title, tbl_Product_1.Prod_name, 
                         tbl_Product_1.Barcode, 1 AS SNO, tbl_Purchase.Purch_bill_no, tbl_Purchase.Remarks, tbl_Purchase_Detail.Bns, tbl_Purchase_Detail.Disct, '' AS Expr1, 'Purchase' AS Expr2
FROM            tbl_Purchase INNER JOIN
                         tbl_Purchase_Detail ON tbl_Purchase.Purch_id = tbl_Purchase_Detail.Purch_id INNER JOIN
                         tbl_Product AS tbl_Product_1 ON tbl_Purchase_Detail.Prod_id = tbl_Product_1.Prod_id INNER JOIN
                         tbl_Unit AS tbl_Unit_1 ON tbl_Product_1.Unit_id = tbl_Unit_1.Unit_id INNER JOIN
                         TblAccEntry ON tbl_Purchase.Contact_id = TblAccEntry.EntryAccID
WHERE        (tbl_Purchase.BranchID = @Comp) AND ('P'+CONVERT(VARCHAR,tbl_Purchase.Purch_id) = @BillNo)</CommandText>
