                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        MAX('Cost of Goods Sold') AS AccCode, SUM(ISNULL(Pur_Rate.P_Rate, tbl_Product.p_Price) * tbl_SaleDetail.prod_qty) AS Debit, MAX(0) AS Credit, SUM(ISNULL(Pur_Rate.P_Rate, tbl_Product.p_Price) * tbl_SaleDetail.prod_qty) 
                         AS Balac, MAX('Inventory Purchases') AS AccHeadName, tbl_Product.p_Price, SUM(tbl_SaleDetail.prod_qty) AS saleqty, tbl_SaleDetail.Prod_rate, SUM(ISNULL(Pur_Rate.P_Rate, tbl_Product.p_Price) * tbl_SaleDetail.Bns) 
                         AS Schm, SUM(tbl_SaleDetail.SPDisct) AS SPDisct, SUM(ISNULL(tbl_SaleDetail.Disct, 0) * tbl_SaleDetail.Prod_rate * tbl_SaleDetail.prod_qty / 100) AS Dist, Pur_Rate.P_Rate, 
                         SUM(tbl_SaleDetail.Prod_rate * tbl_SaleDetail.prod_qty) AS SaleA, tbl_Product.Prod_name
FROM            tbl_SaleDetail INNER JOIN
                         tbl_Sale ON tbl_SaleDetail.Sale_id = tbl_Sale.Sale_id INNER JOIN
                         tbl_Product ON tbl_SaleDetail.prod_id = tbl_Product.Prod_id LEFT OUTER JOIN
                             (SELECT        tbl_Purchase_Detail_2.Prod_id, CASE WHEN Prod_id = 12 THEN 1.0126 ELSE SUM(tbl_Purchase_Detail_2.Prod_rate * tbl_Purchase_Detail_2.Prod_qty * ISNULL(tbl_Paking_3.Pakg_no, 1)) 
                                                         / SUM(tbl_Purchase_Detail_2.Prod_qty * ISNULL(tbl_Paking_3.Pakg_no, 1)) END AS P_Rate
                               FROM            tbl_Purchase AS tbl_Purchase_2 INNER JOIN
                                                         tbl_Purchase_Detail AS tbl_Purchase_Detail_2 ON tbl_Purchase_2.Purch_id = tbl_Purchase_Detail_2.Purch_id LEFT OUTER JOIN
                                                         tbl_Paking AS tbl_Paking_3 ON tbl_Purchase_Detail_2.Pakg_id = tbl_Paking_3.Pakg_id
                               WHERE        (tbl_Purchase_2.Purch_date &lt;= CONVERT(DATETIME, @D2, 102)) AND (tbl_Purchase_2.CompID = @CId) AND (tbl_Purchase_2.InvoiceType = 1)
                               GROUP BY tbl_Purchase_Detail_2.Prod_id) AS Pur_Rate ON tbl_SaleDetail.prod_id = Pur_Rate.Prod_id
WHERE        (tbl_Sale.Sale_date BETWEEN CONVERT(DATETIME, @D1, 102) AND CONVERT(DATETIME, @D2, 102)) AND (tbl_Sale.CompID = @CId)
GROUP BY tbl_Product.Prod_name, tbl_Product.p_Price, tbl_SaleDetail.Prod_rate, Pur_Rate.P_Rate
HAVING        (SUM(ISNULL(Pur_Rate.P_Rate, tbl_Product.p_Price) * tbl_SaleDetail.prod_qty) IS NOT NULL)</CommandText>
