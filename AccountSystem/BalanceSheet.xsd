                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TblAccEntry.EntryAccName AS AccCode, SUM(VW_VoucherDTL.AmountDeb) AS Debit, SUM(VW_VoucherDTL.AmountCre) AS Credit, TblAccHead.AccHeadName, 
                         CASE WHEN SUM(VW_VoucherDTL.AmountDeb) - SUM(VW_VoucherDTL.AmountCre) &lt;= (SUM(VW_VoucherDTL.AmountDeb) - SUM(VW_VoucherDTL.AmountCre)) 
                         * - 1 THEN 0 ELSE SUM(VW_VoucherDTL.AmountDeb) - SUM(VW_VoucherDTL.AmountCre) END AS Balac
FROM            TblCompany INNER JOIN
                         TblAccHead ON TblCompany.CompanyID = TblAccHead.CompanyID INNER JOIN
                         TblAccSubHead ON TblAccHead.AccHeadID = TblAccSubHead.AccHeadID INNER JOIN
                         TblAccEntry ON TblAccSubHead.AccSubID = TblAccEntry.AccSubID INNER JOIN
                         VW_VoucherDTL ON TblAccEntry.EntryAccID = VW_VoucherDTL.AccEntryID INNER JOIN
                         VW_VoucherHDR ON VW_VoucherDTL.VoucherID = VW_VoucherHDR.VoucherID
WHERE        (TblCompany.CompanyID = @Cid) AND (VW_VoucherHDR.EntryDate &lt;= @d2) AND (TblAccHead.AccType &lt;&gt; N'revenue') AND 
                         (TblAccHead.AccType &lt;&gt; N'expenditure')
GROUP BY TblAccEntry.EntryAccName, TblAccHead.AccHeadName
HAVING        (SUM(VW_VoucherDTL.AmountDeb - VW_VoucherDTL.AmountCre) &lt;&gt; 0)</CommandText>
